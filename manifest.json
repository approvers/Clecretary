{
  "name": "Clecretary",
  "version": "0.1",
  "description": "Manage assignments in google classroom",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "js": ["contentScript.js"],
    "matches": ["https://classroom.google.com/u/*/a/not-turned-in/all"]
  }],
  "permissions": ["webNavigation"]
}
